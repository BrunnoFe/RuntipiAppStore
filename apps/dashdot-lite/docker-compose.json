{
  "version": "3.5",
  "services": [
    {
      "name": "dashdot-lite",
      "image": "mauricenino/dashdot:6.2.4",
      "container_name": "dashdot_lite",
      "isMain": true,
      "restart": "unless-stopped",
      "internalPort": 3001,
      "privileged": true,
      "environment":{
        "DASHDOT_SHOW_HOST":"false",
        "DASHDOT_SHOW_DASH_VERSION":"true",
        "DASHDOT_ENABLE_CPU_TEMPS":"true",
        "DASHDOT_USE_IMPERIAL":"true",
        "DASHDOT_ALWAYS_SHOW_PERCENTAGES":"true",
        "DASHDOT_PAGE_TITLE":"dashdot",
        "DASHDOT_ACCEPT_OOKLA_EULA":"true"
      },
      "volumes": [
        {
          "hostPath": "/",
          "containerPath": "/mnt/host",
          "readOnly": true
        }
      ],
      "deploy": {
        "resources": {
          "limits": {
            "cpus": "0.5"
          }
        }
      },
      "cpu_shares":"128",
      "networks": [
        "tipi_main_network"
      ],
      "labels": {
        "runtipi.managed": "true",
        "traefik.enable": "true",
        "traefik.http.middlewares.dashdot-lite-web-redirect.redirectscheme.scheme": "https",
        "traefik.http.services.dashdot-lite.loadbalancer.server.port": "3001",
        "traefik.http.routers.dashdot-lite-insecure.rule": "Host(`${APP_DOMAIN}`)",
        "traefik.http.routers.dashdot-lite-insecure.entrypoints": "web",
        "traefik.http.routers.dashdot-lite-insecure.service": "dashdot-lite",
        "traefik.http.routers.dashdot-lite-insecure.middlewares": "dashdot-lite-web-redirect",
        "traefik.http.routers.dashdot-lite.rule": "Host(`${APP_DOMAIN}`)",
        "traefik.http.routers.dashdot-lite.entrypoints": "websecure",
        "traefik.http.routers.dashdot-lite.service": "dashdot-lite",
        "traefik.http.routers.dashdot-lite.tls.certresolver": "myresolver",
        "traefik.http.routers.dashdot-lite-local-insecure.rule": "Host(`dashboard-lite.${LOCAL_DOMAIN}`)",
        "traefik.http.routers.dashdot-lite-local-insecure.entrypoints": "web",
        "traefik.http.routers.dashdot-lite-local-insecure.service": "dashdot-lite",
        "traefik.http.routers.dashdot-lite-local-insecure.middlewares": "dashdot-lite-web-redirect",
        "traefik.http.routers.dashdot-lite-local.rule": "Host(`dashdot-lite.${LOCAL_DOMAIN}`)",
        "traefik.http.routers.dashdot-lite-local.entrypoints": "websecure",
        "traefik.http.routers.dashdot-lite-local.service": "dashdot-lite",
        "traefik.http.routers.dashdot-lite-local.tls": "true"
      }
    }
  ]
}